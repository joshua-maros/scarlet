
Integer32 is rover::lang::Integer32
sum is Integer32::sum
difference is Integer32::difference

rover::lang::Integer32::sum::left info{}

index is any{Integer32}

fibb is 
pick{
   if   index is_variant{0}, 1
   elif index is_variant{1}, 1
   else sum[
      fibb[difference[index 1]] 
      fibb[difference[index 2]]
   ]
}
ix Integer32 From{index}

fibb[8] info{}


#=
recount is 
pick{
   if input == 0, 0
   else           recount[input - 1]
}
where{
   input is any{Integer32}
}
=#
