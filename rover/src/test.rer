TYPE is rover::lang::TYPE
I32 is rover::lang::Integer32
zero is rover::core::zero
one is rover::core::one

var is any{I32}

Point is Type{
   new is Self recording{
      x is any{I32} 
      y is any{I32}
   }
}

x is Point::new::x
y is Point::new::y

point is Point::new 
replacing{
   x with zero
   y with one
}

# asdf asdf asfd 0123

#=
recount is 
pick{
   if input == 0, 0
   else           recount[input - 1]
}
where{
   input is any{Integer32}
}
=#
