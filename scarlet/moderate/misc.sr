logic IS std.logic
true IS std.true
false IS std.false
void IS std.void
x IS std.basics.x

# x IS VAR[]

x.IS_POPULATED_STRUCT.SHOWN

is_struct IS 
# logic.or[
   logic.and[
      x.IS_POPULATED_STRUCT
      is_struct[x IS x.REST]
   ]
#    x = void
# ]

ASSERT[is_struct[x IS x.REST] FROM logic.Bool]
