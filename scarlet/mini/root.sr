defining{
    TYPE is builtin_item{TYPE}

    defining{
        zero is variant_of{Bit}
        one is variant_of{Bit}

        defining{
            x is any{Bit}
        }
        not is x
        matching{
            on{Bit::zero} Bit::one
            on{Bit::one} Bit::zero
        }
    }
    Bit is variant_of{TYPE}
}
Bit::not[Bit::not::x is Bit::zero] displayed{}
