defining{
    TYPE is builtin_item{TYPE}

    defining{
        zero is variant_of{Bit}
        one is variant_of{Bit}
    }
    Bit is variant_of{TYPE}

    x is any{Bit}
    not is 
    x matching{
        on{Bit::zero} Bit::one
        on{Bit::one} Bit::zero
    }

    identity is 
    not[not[not]]
    displayed{}
}
