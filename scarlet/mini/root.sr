{
    PATTERN is Builtin(PATTERN)
    # BOOL is Builtin(BOOL)
    32U is Builtin(32U)

    length is 32U.V.Eager(All)
    element_type is PATTERN.V.Eager(All)
    ARRAY is Builtin(ARRAY length element_type)

    STRING is {
        length is 32U.V
        content is ARRAY(length 32U)
    }

    STRING.Shown
}
